{
    "name": "DevGET",
    "version": "1.0",
    "description": "Scrapes devpost and generates csvs for teams for judging",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage",
        "https://*.devpost.com/"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}
